{
  "name": "get-calendar-dates",
  "version": "0.0.0",
  "description": "Get all ISO calendar dates in a string",
  "repository": "shinnn/get-calendar-dates",
  "scripts": {
    "bower": "bower install --production",
    "pretest": "jscs *.js && eslint *.js && ${npm_package_scripts_bower}",
    "test": "node test.js | tap-spec",
    "coverage": "istanbul cover -x=bower_components/*/*.js test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/shinnn/get-calendar-dates/blob/master/LICENSE"
    }
  ],
  "files": [
    "index.js"
  ],
  "keywords": [
    "match",
    "string",
    "search",
    "date",
    "year",
    "month",
    "day",
    "calendar",
    "iso",
    "iso8601",
    "browser",
    "client-side"
  ],
  "dependencies": {
    "calendar-date-regex": "^1.0.0"
  },
  "devDependencies": {
    "bower": "^1.3.12",
    "eslint": "^0.13.0",
    "istanbul": "^0.3.5",
    "istanbul-coveralls": "^1.0.1",
    "jscs": "^1.10.0",
    "require-bower-files": "^2.0.0",
    "tap-spec": "^2.2.0",
    "tape": "^3.4.0"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "validateLineBreaks": "LF"
  }
}
