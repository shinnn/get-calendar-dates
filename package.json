{
  "name": "get-calendar-dates",
  "version": "1.0.0",
  "description": "Get all ISO calendar dates in a string",
  "repository": "shinnn/get-calendar-dates",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "bower": "bower install --production",
    "pretest": "eslint --fix --config @shinnn browser.js index.js test.js && ${npm_package_scripts_bower}",
    "test": "node --throw-deprecation --track-heap-objects test.js | tap-spec",
    "coverage": "istanbul cover -x=bower_components/*/*.js test.js"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords": [
    "match",
    "string",
    "search",
    "date",
    "year",
    "month",
    "day",
    "calendar",
    "iso",
    "iso8601",
    "browser",
    "client-side"
  ],
  "dependencies": {
    "calendar-date-regex": "^1.0.1"
  },
  "devDependencies": {
    "@shinnn/eslint-config": "^3.1.0",
    "bower": "^1.7.9",
    "eslint": "^3.4.0",
    "istanbul": "^0.4.5",
    "require-bower-files": "^3.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  }
}
