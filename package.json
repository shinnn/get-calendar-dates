{
  "name": "get-calendar-dates",
  "version": "1.0.0",
  "description": "Get all ISO calendar dates in a string",
  "repository": "shinnn/get-calendar-dates",
  "author": "Shinnosuke Watanabe (https://github.com/shinnn)",
  "scripts": {
    "bower": "bower install --production",
    "pretest": "eslint --config node_modules/@shinnn/eslintrc/rc.json browser.js index.js test.js && ${npm_package_scripts_bower}",
    "test": "node --harmony_arrow_functions test.js | tap-spec",
    "coverage": "node --harmony_arrow_functions node_modules/.bin/istanbul cover -x=bower_components/*/*.js test.js",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords": [
    "match",
    "string",
    "search",
    "date",
    "year",
    "month",
    "day",
    "calendar",
    "iso",
    "iso8601",
    "browser",
    "client-side"
  ],
  "dependencies": {
    "calendar-date-regex": "^1.0.1"
  },
  "devDependencies": {
    "@shinnn/eslintrc": "^1.0.0",
    "bower": "^1.4.1",
    "eslint": "^0.24.0",
    "istanbul": "^0.3.17",
    "istanbul-coveralls": "^1.0.3",
    "require-bower-files": "^2.0.0",
    "tap-spec": "^4.0.2",
    "tape": "^4.0.0"
  }
}
